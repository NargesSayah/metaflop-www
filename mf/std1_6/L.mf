% MetaFlop
% Standard 1.3

beginadjchar("L",5, cap#, 0);


x1=x2=leftstemloc;
x1r=x2r=x1+(cont*px/2);
x1=x5;
x6=w-x1l;

y1=0;
y2=h;
bot y5=0;
bot y6=0;
y5l=y6l=y5-px/2;

penpos1(whatever,0);
penpos2(whatever,0);
penpos5(whatever,90);
penpos6(whatever,-90);

penstroke z1e..z2e;
penstroke z5e..z6e;

penlabels(range 1 thru 30);

endchar;
