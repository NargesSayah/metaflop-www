% MetaFlop
% Standard 1.3

beginadjchar("T",5, cap#, 0);

% normal_adjust_fit(-0.25u#,-0.25u#);




x5=leftstemloc-(cont*px)/2;
x6=w-x5;



x1=x2=.5w;

y1=0;
y2=h;


top y5=h;
y5=y6;


penpos1(cont*px,0);
penpos2(cont*px,0);
penpos5(px,90);
penpos6(px,90);

penstroke z1e..z2e;
penstroke z5e..z6e;

penlabels(range 1 thru 30);





endchar;
